{
	"manifest_version": 2,
	"name": "Imperial",
	"version": "1.0",
	"applications": {
		"gecko": {
			"strict_min_version": "56.0a1"
		}
	},
 
	"description": "Injects text into webpage to show imperial equivalents to metric units." ,
	
	
	"browser_action": {
		"default_title": "Imperial"
	},

	"icons": {
		"16": "icons/imp-16.png",
		"32": "icons/imp-32.png",
		"48": "icons/imp-48.png"
	},
	
	"content_scripts": [
    {
		"matches": ["<all_urls>"],
		"js": ["search_text.js"]
    }]
}